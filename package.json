{
  "name": "react-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "gen": "buf generate --template proto/buf.gen.yaml proto && node scripts/fix-imports.mjs",
    "build:server": "node scripts/build-server.mjs",
    "build:server:watch": "node scripts/build-server.mjs --watch",
    "dev": "pnpm gen && pnpm build:server && concurrently \"pnpm dev:vite\" \"pnpm dev:server\"",
    "dev:vite": "vite",
    "dev:server": "concurrently \"pnpm build:server:watch\" \"node scripts/sleep.mjs 2 && node --watch dist/server.mjs\"",
    "build": "pnpm gen && pnpm build:server && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node dist/server.mjs",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test:api": "hurl --test --variables-file tests/.env tests/api/*.hurl"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@bufbuild/protobuf": "^1.10.0",
    "@connectrpc/connect": "^1.7.0",
    "@connectrpc/connect-express": "^1.7.0",
    "@connectrpc/connect-node": "^1.7.0",
    "@connectrpc/connect-web": "^1.7.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query": "^5.90.14",
    "better-auth": "^1.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "lucide-react": "^0.562.0",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.61.0",
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "@connectrpc/protoc-gen-connect-es": "1.7.0",
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-node": "^5.2.0"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
